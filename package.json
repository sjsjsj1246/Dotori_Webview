  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@next/bundle-analyzer": "^10.0.5",
    "antd": "^4.10.2",
    "axios": "^0.21.1",
    "next": "12.0.7",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.5.16",
    "react-use": "^15.3.8",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "zustand": "^3.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.14",
